@page "/"
@namespace BlazorSandbox.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>BlazorSandbox</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="vendors/pickadate.compressed/themes/classic.css" rel="stylesheet" />
    <link href="vendors/pickadate.compressed/themes/classic.date.css" rel="stylesheet" />
    <link href="css/site.css" rel="stylesheet" />
</head>
<body>
    <app>
        <component type="typeof(App)" render-mode="ServerPrerendered" />
    </app>

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="_framework/blazor.server.js"></script>
    <script src="vendors/jquery-3.4.1.min.js"></script>
    <script src="vendors/pickadate.compressed/picker.js"></script>
    <script src="vendors/pickadate.compressed/picker.date.js"></script>
    <script src="vendors/jquery.maskedinput.min.js"></script>
    <script src="js/app.js"></script>
</body>
</html>
